{
    "name": "react-datepicker",
    "version": "1.0.0",
    "description": "A react datepicker package",
    "author": "colinluzio",
    "license": "Apache-2.0",
    "main": "dist/index.js",
    "engines": {
        "node": ">=8",
        "npm": ">=5"
    },
    "scripts": {
        "build": "rollup -c",
        "start": "rollup -c -w",
        "test": "jest"
    },
    "dependencies": {
        "@babel/polyfill": "^7.10.4",
        "@testing-library/jest-dom": "^5.11.2",
        "@types/jsdom-global": "^3.0.2",
        "@types/react-test-renderer": "^16.9.3",
        "@types/testing-library__jest-dom": "^5.9.2",
        "jest-dom": "^4.0.0",
        "jest-transform-css": "^2.0.0",
        "ts-loader": "^8.0.2",
        "typescript": "^3.9.7"
    },
    "peerDependencies": {
        "react": ">= 16.8.0",
        "react-dom": ">= 16.8.0"
    },
    "devDependencies": {
        "@types/enzyme-adapter-react-16": "^1.0.6",
        "@types/jest": "^26.0.9",
        "@types/react": "^16.3.13",
        "@types/react-dom": "^16.0.5",
        "babel-core": "^6.26.3",
        "babel-runtime": "^6.26.0",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.3",
        "jest": "^26.2.2",
        "jsdom": "16.4.0",
        "jsdom-global": "3.0.2",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "rollup": "^1.32.1",
        "rollup-plugin-sass": "^1.2.2",
        "rollup-plugin-typescript2": "^0.25.3",
        "ts-jest": "^26.1.4",
        "webpack": "^4.44.1",
        "webpack-cli": "^3.3.12"
    },
    "overrides": {
        "*": {
            "dependencies": {
                "react": "-"
            },
            "peerDependencies": {
                "react": "+"
            }
        }
    },
    "files": [
        "dist"
    ],
    "keywords": [
        "react",
        "typescript",
        "npm"
    ],
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node",
        "transform": {
            "^.+\\.js$": "babel-jest",
            ".+\\.(css|styl|less|sass|scss)$": "jest-transform-css"
        },
        "testPathIgnorePatterns": [
            "domManipulators.ts"
        ]
    }
}
